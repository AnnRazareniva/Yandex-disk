<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="1000.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.yandexdisk.Controller">
   <Pane prefHeight="120.0" prefWidth="1020.0">
      <children>
         <ImageView fitHeight="57.0" fitWidth="63.0" layoutX="903.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
         <ImageView fitHeight="66.0" fitWidth="57.0" layoutX="14.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@../../../YandexDisk.png" />
            </image>
         </ImageView>
      </children>
      <VBox.margin>
         <Insets />
      </VBox.margin>
      <padding>
         <Insets left="10.0" />
      </padding>
   </Pane>
   <VBox alignment="CENTER" prefHeight="425.0" prefWidth="980.0">
      <children>
          <Button fx:id="Upload" alignment="CENTER" onAction="#OnUploadButtonClick" text="Загрузить файл на Яндекс Диск">
            <padding>
               <Insets bottom="20.0" left="55.0" right="55.0" top="20.0" />
            </padding>
            <VBox.margin>
               <Insets bottom="35.0" />
            </VBox.margin>
          </Button>
         <Label text="Или скачать файл по ссылке:" textFill="#707b91">
            <font>
               <Font size="15.0" />
            </font>
            <VBox.margin>
               <Insets bottom="20.0" top="35.0" />
            </VBox.margin>
         </Label>
         <TextField maxHeight="20.0" maxWidth="500.0" prefHeight="25.0" prefWidth="500.0">
            <VBox.margin>
               <Insets bottom="20.0" />
            </VBox.margin></TextField>
         <Button fx:id="Download" mnemonicParsing="false" onAction="#OnDownloadButtonClick" prefHeight="40.0" prefWidth="110.0" text="Скачать" />
      </children>
      <padding>
         <Insets bottom="30.0" />
      </padding>
   </VBox>
   <padding>
      <Insets left="20.0" top="20.0" />
   </padding>
</VBox>
